@{
    ViewData["Title"] = "Alle Dokumenter";
}
<p></p>

<div class="text-center mx-auto pb-5 wow fadeIn" data-wow-delay=".3s" style="max-width: 600px;">
    <h1 class="text-primary">Alle Dokumenter</h1>
    <h5 class="text-black">Fra ejendommen</h5>
</div>


@if (Model.Count > 0)
{
    <table class="table">
        <thead>
        <tr>
            <th>Titel</th>
           
        </tr>
        </thead>
        <tbody>
        @foreach (var document in Model)
        {
            <tr>
                <td>@document.Title</td>
            </tr>
        }
        </tbody>
    </table>
}
else
{
    <p>Ingen dokumenter fundet.</p>
}
